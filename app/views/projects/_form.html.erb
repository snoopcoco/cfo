<%= form_for(@project) do |f| %>
<%= render 'shared/error_messages', object: f.object %>
  
<%# プロジェクト登録内容　%>
  <%= f.label :pj_type, "プロジェクトタイプ：" %>
  <%= f.select :pj_type, ["asoview", "bizdev", "media"] %>
  
  <%= f.label :pj_name, "プロジェクト名：" %>
  <%= f.text_field :pj_name %>

  <%= f.label :pj_detail, "プロジェクト名：" %>
  <%= f.text_field :pj_detail %>
  
  <%= f.field_for :tasks do |task| %>
  <%= render 'task', locals: {task: task } %>
  
  <%= f.submit class: "btn btn-sm btn-primary" %>
<% end %>
<%# プロジェクト登録内容 end　%>